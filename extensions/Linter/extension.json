					"enabled": true,
					"priority": "low",
					"has-name": true
				},
				"self-closed-tag": {
					"dbid": 6,
					"enabled": true,
					"priority": "high",
					"has-name": true,
					"parser-migration": true
				},
				"deletable-table-tag": {
					"dbid": 7,
					"enabled": true,
					"priority": "high",
