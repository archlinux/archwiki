	{{/ip64Link}}
	<span class="mw-changeslist-links">{{{blockLink}}}</span>
	<span class="mw-changeslist-links">{{timeRange}}</span>
	<strong class="mw-checkuser-actions-count mw-checkuser-edits-count">{{editCount}}</strong>
	{{#showIpCounts}}
