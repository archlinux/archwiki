{
  "name": "ArchLinux",
  "author": [
    "Pierre Schmitz"
  ],
  "url": "https://www.archlinux.org",
  "description": "Arch Linux navigation and styles",
  "license-name": "GPL-2.0+",
  "type": "skin",
  "AutoloadClasses": {
    "MediaWiki\\Extensions\\ArchLinux\\Hooks": "ArchLinux.hooks.php",
    "OOUI\\ArchLinuxTheme": "oojs-ui/php/themes/ArchLinuxTheme.php"
  },
  "Hooks": {
    "BeforePageDisplay": [
      "MediaWiki\\Extensions\\ArchLinux\\Hooks::onBeforePageDisplay"
    ],
    "AfterFinalPageOutput": [
      "MediaWiki\\Extensions\\ArchLinux\\Hooks::onAfterFinalPageOutput"
    ]
  },
  "ResourceModules": {
    "zzz.ext.archLinux.styles": {
      "targets": [
        "mobile",
        "desktop"
      ],
      "styles": [
        "archnavbar/archnavbar.less",
        "archnavbar/responsive.less",
        "arch_common.less"
      ],
      "skinStyles": {
        "vector": "skins/vector.less",
        "vector-2022": "skins/vector.less",
        "monobook": "skins/monobook.less"
      },
      "position": "top"
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "modules",
    "remoteExtPath": "ArchLinux/modules"
  },
  "SkinOOUIThemes": {
    "timeless": "ArchLinux",
    "vector": "ArchLinux",
    "vector-2022": "ArchLinux",
    "monobook": "ArchLinux"
  },
  "OOUIThemePaths": {
    "ArchLinux": {
      "localBasePath": "oojs-ui/dist",
      "remoteSkinPath": "ArchLinux/oojs-ui/dist",
      "scripts": "oojs-ui-archlinux.js",
      "styles": "oojs-ui-{module}-archlinux.css",
      "images": "themes/archlinux/{module}.json"
    }
  },
  "ConfigRegistry": {
    "archlinux": "GlobalVarConfig::newInstance"
  },
  "config": {
    "ArchNavBar": {
      "value": []
    },
    "ArchHome": {
      "value": ""
    },
    "ArchNavBarSelected": {
      "value": []
    },
    "ArchNavBarSelectedDefault": {
      "value": ""
    }
  },
  "manifest_version": 2
}
