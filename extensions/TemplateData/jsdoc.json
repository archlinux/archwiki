{
	"opts": {
		"destination": "docs",
		"package": "package.json",
		"pedantic": true,
		"recurse": true
	},
	"plugins": [
		"plugins/markdown"
	],
	"source": {
		"include": [ "modules", "modules/README.md" ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		}
	}
}
