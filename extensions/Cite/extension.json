			"localBasePath": "modules",
			"remoteExtPath": "Cite/modules",
			"scripts": [
				"ext.cite.a11y.js",
				"ext.cite.highlighting.js",
				"ext.cite.tracking.js"
			],
			"styles": [
				"ext.cite.a11y.css",
				"ext.cite.highlighting.css"
