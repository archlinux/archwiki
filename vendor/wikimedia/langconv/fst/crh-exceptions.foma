load defined crh-exceptions.bin
#define CRH'LATN'EXCEPTIONS CRH'LATN'EXCEPTIONS'defer(?);
#define CRH'CYRL'EXCEPTIONS CRH'CYRL'EXCEPTIONS'defer(?);

# Some of these exceptions include spaces, which would break the word-based
# bracketing function.  Hack around that by treating the space-containing
# exceptions as a single "word", as well as the trailing period after
# single-character roman numerals.
define CRH'HAS'SPACE [\BRACKETS]* { } [\BRACKETS]*;
define MAPPINGS'LATN MULTICASE'MAPPINGS'LATN | EXACTCASE'MAPPINGS'LATN;
define MAPPINGS'CYRL MULTICASE'MAPPINGS'CYRL | EXACTCASE'MAPPINGS'CYRL;
# the convert functions aren't defined yet, so make this a function to avoid
# use-before-def
define CRH'WORD'HELPER(convert'crh'cyrl,convert'crh'latn) [
 ALL'WORD'CHARS |
 [CRH'HAS'SPACE .o. MAPPINGS'LATN].u |
 [CRH'HAS'SPACE .o. convert'crh'cyrl .o. MAPPINGS'LATN].u |
 [CRH'HAS'SPACE .o. MAPPINGS'CYRL].u |
 [CRH'HAS'SPACE .o. convert'crh'latn .o. MAPPINGS'CYRL].u |
 [ {M}|{D}|{C}|{L}|{X}|{V}|{I} ] "." |
 [NO'BRACKETS .o. convert'crh'latn .o. convert'crh'cyrl .o. {№}].u
]+;

