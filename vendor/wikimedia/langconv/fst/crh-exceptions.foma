# From CrhExceptions.php

define MY'UC [ [ L'LC'UC | C'LC'UC ] .P. ? ]* ;
define MY'LC [ [ L'UC'LC | C'UC'LC ] .P. ? ]* ;
define CREATE'UC'MAP(lc'map) [
  MY'UC.l .o. MY'LC .o. lc'map .o. MY'UC
];
define CREATE'UCWORD'MAP(lc'map) [
  [[MY'UC.l & ?] MY'LC.l ] .o. MY'LC .o. lc'map .o. [[?.o.MY'UC] IDENTITY]
];

