{
	"config": {
		"autoloader-suffix": "_mediawiki_vendor",
		"classmap-authoritative": true,
		"optimize-autoloader": true,
		"prepend-autoloader": false,
		"vendor-dir": ".",
		"sort-packages": true,
		"allow-plugins": {
			"wikimedia/composer-merge-plugin": true
		}
	},
	"prefer-stable": true,
	"require": {
		"php": ">=7.3.19",
		"composer-plugin-api": "^2.0",
		"christian-riesen/base32": "1.6.0",
		"composer/semver": "3.2.6",
		"cssjanus/cssjanus": "2.1.0",
		"guzzlehttp/guzzle": "7.4.1",
		"guzzlehttp/promises": "1.5.1",
		"guzzlehttp/psr7": "2.1.2",
		"jakobo/hotp-php": "2.0.0",
		"justinrainbow/json-schema": "5.2.11",
		"liuggio/statsd-php-client": "1.0.18",
		"monolog/monolog": "2.2.0",
		"oojs/oojs-ui": "0.43.2",
		"opis/json-schema": "1.1.0",
		"pear/console_getopt": "1.4.3",
		"pear/mail": "1.4.1",
		"pear/mail_mime": "1.10.11",
		"pear/net_smtp": "1.10.0",
		"pear/net_socket": "1.2.2",
		"pear/net_url2": "2.2.2",
		"pear/pear-core-minimal": "1.10.11",
		"pear/pear_exception": "1.0.2",
		"psr/container": "1.1.1",
		"psr/http-client": "1.0.1",
		"psr/http-factory": "1.0.1",
		"psr/http-message": "1.0.1",
		"psr/log": "1.1.4",
		"psr/simple-cache": "1.0.1",
		"ralouphie/getallheaders": "3.0.3",
		"symfony/polyfill-php80": "1.25.0",
		"symfony/polyfill-php81": "1.25.0",
		"symfony/yaml": "5.4.3",
		"wikimedia/assert": "0.5.1",
		"wikimedia/at-ease": "2.1.0",
		"wikimedia/base-convert": "2.0.1",
		"wikimedia/cdb": "2.0.0",
		"wikimedia/cldr-plural-rule-parser": "2.0.0",
